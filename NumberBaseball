import random
a = [0,0,0]
a[0] = random.randint(0,9)
a[1] = random.randint(0,9)
a[2] = random.randint(0,9)
b = [0,0,0]
i = 0
b[0],b[1],b[2] = input("3 numbers:").split(" ")
b[0] = int(b[0])
b[1] = int(b[1])
b[2] = int(b[2])
while (i<3):
    if (b[i]==a[i]):
        print("%d : strike"%(i+1))
    elif (b[i] in a):
        print("%d : ball"%(i+1))
    else:
        print("%d : out"%(i+1))
    i+=1
